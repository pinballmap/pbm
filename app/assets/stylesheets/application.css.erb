    font-weight: normal;
    list-style-position: inside;
    padding: 0 0 0 10px;
}
.score_result {
    background-color: #DBD9D7;
    width: 700px;
}
#results_container {
    color: #180E03;
    height: 100%;
    width: 380px;
    z-index: 50;
}
.scroll_pane {
    padding-left: 20px;
    width: 350px;
}
.location_summary_image_mouseover {
    background-image: url("/assets/coincard-c60f3b46d2904227a4f8bbf3033276e0.png");
    background-position: center center;
    background-repeat: no-repeat;
}
.show_location_detail_location {
    border-bottom: 2px solid #B59C9A;
    padding-bottom: 10px;
}
.search_option_selector a {
    color: #000000;
    padding-right: 10px;
}
.delete_button {
    background-color: black;
    border: 1px solid #494949;
    color: #494949;
    cursor: pointer;
    margin-left: 5px;
    padding: 0 1px;
}
.show_machines_location {
    padding-top: 2px;
    width: 310px;
}
.lookup_search_input {
    width: 275px;
}
.lookup_search_select {
    width: 280px;
}
#file_chooser_button {
    padding-left: 10px;
    width: 90px;
}
.location_detail {
    border-bottom: 2px solid #B59C9A;
    cursor: pointer;
    font-size: 10px;
    font-weight: normal;
    height: 100%;
    padding: 3px 10px 10px 0;
    width: 310px;
}
.location_description {
    color: #888888;
    font-style: italic;
    padding-left: 10px;
    padding-top: 5px;
}
.desc_show_location {
    padding-bottom: 5px;
    padding-top: 5px;
    width: 101%;
}
.desc_show_location:hover {
    background-color: #DBD9D7;
}
.photo_image {
    border-color: white;
    border-style: solid;
    border-width: 6px;
}
.photo_caption {
    font-style: italic;
}
.blue_header {
    color: #A9BEFF;
    font-size: 14px;
}
#findapinball {
    font-size: 16px;
    text-align: center;
}
.question {
    color: #FFFFCC;
    font-size: 14px;
    font-weight: bold;
}
.answer {
    font-size: 14px;
}
