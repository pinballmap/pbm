/*
 *= require jquery-ui
 */

@import url(https://fonts.googleapis.com/css?family=Fira+Mono:400,700);

/** Core **/

@font-face {
  font-family: 'JohnDoe';
  src: url(<%= asset_path "JohnDoe.otf" %>);
}

* {
  outline: none;  
}
body {
  margin: 0 auto;
  padding: 0px;
  background-image: url(<%= asset_path 'PPM-SplashTile-BW-Inverted.png' %>);
  background-color: #FFFFFF;
  background-size: 4px auto;
  color: #2e2322;
  font-family: 'JohnDoe',Helvetica,Arial,sans-serif;
  font-weight: bold;
  -webkit-font-smoothing: antialiased;
}
p {
  font-size: 16px;
  line-height: 20px;
}
p a {
  background-color: #ebeae9;
}
img {
  border: 0;
}
h1 {
  font-size: 36px;
}
a {
  color: #441e56;
  text-decoration: none;
  border: 0;
}
a:hover {
  text-decoration: underline;
  color: #58266F;
}
input, textarea {
  -webkit-appearance: none;
  -webkit-border-radius: 0;
  -moz-border-radius: 0;
  border-radius: 0;
  -webkit-box-sizing: border-box; /* Safari/Chrome, other WebKit */
  -moz-box-sizing: border-box;    /* Firefox, other Gecko */
  box-sizing: border-box;         /* Opera/IE 8+ */
  font-family: "Fira Mono",Helvetica,Arial,sans-serif;
}
select {
	font-family: Helvetica,Arial,sans-serif;
}
ul {
  font-size: 16px;
}
li {
  padding: 2px 0 6px 4px;
}
li a:hover {
  text-decoration: underline;
}
ul li li {
  list-style-type: decimal;
}
.notbold {
  font-weight: normal;
}
.subpage_text {
  font-size: 14px;
  letter-spacing: .3px;
}
.pink {
  color: #ffcc00;
  font-size: 14px;
  font-weight: bold;
}

/* Text */

.intro {
  font-size: 16px;
}
.dark {
  color: #3a3835;
}
.darkb {
  color: #43403d;
  font-weight: bold;
  text-shadow: 0.1px 0.1px 0.1px white;
  font-family: "JohnDoe";
}
.italic {
  font-style: italic;
}
.bold {
  font-weight: bold;
}
.hello {
  font-size: 28px;
  line-height: 36px;
  text-shadow: 1px 1px 0 rgba(255, 255, 255, 0.8);
  margin: 0;
  font-weight: 700;
}

/** END Core **/

/** Login **/

#login {
  background: none repeat scroll 0 0 rgba(253, 252, 254, 0.7);
  border: 2px solid #E3D6C5;
  margin: 70px auto 0;
  padding: 0 20px 20px;
  width: 270px;
}
#login label.password,
#login label.email {
  display: block;
}
#login label.boolean {
  font-size: 12px;
}
#login input.boolean {
  height: 16px;
}
form.new_user {
  font-size: 16px;
}
#login .form-actions {
  line-height: 40px;
}
#login abbr {
  border: none;
}

/** Home **/

ul.twitter {
  list-style-type: none;
  padding: 0;
  margin-bottom: 0;
}
.twitter li {
  padding: 5px 0;
  border-bottom: 1px solid #DECEB9;
  font-family: "Fira Mono";
  color: #452809;
  font-size: 15px;
}
.twitter li:nth-child(5) { 
  border: none;
}
p.notice {
  font-size: 18px;
  text-align: center;
  margin: -10px auto 10px;
  width: 60%;
}
.thanks, p.alert {
  font-size: 28px;
  line-height: 36px;
  text-align: center;
  margin: 50px auto 0; 
  text-shadow: 0.1em 0.1em 0.2em black;
  color: #FFFFCC;
  background: #e3e1e0;
  border-bottom: 1px solid #c3c0bc;
  border-top: 1px solid #c3c0bc;
  padding: 10px;
}
#flash_error {
  font-size: 16px;
  margin-top: 120px;
  position: fixed;
  background: pink none repeat scroll 0% 0%;
  padding: 10px;
  right: 20px;
  z-index: 4000;
  border: 3px solid black;
}
#flash_alert,
#flash_recaptcha_error {
  display: none;
}
div.pick_a_map {
  font-size: 28px;
  line-height: 42px;
  text-align: center;
  padding-top: 5px;
}
span.pick_a_map, .store_title {
  color: #58266f;
  margin: 0;
  text-shadow: 1px 1px 5px #CCCCCC;
}
div.dont_pick_a_map {
  display: none;
}
div.pick_a_map .apps_header, 
div.pick_a_map .blog_header, 
div.pick_a_map .store_header {
  font-size: 15px;
  line-height: 36px;
  float: left;
  padding: 5px 10px 7px;
  background-color: #ece2d8;
  border-right: 2px solid #d6c0aa;
  text-shadow: 1px 1px 5px #CCCCCC;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
.home_intro {
  padding: 3px 6px 6px;
  background-color: #fdfcfe;
  border: 5px solid #c2a681;
  width: 300px;
  line-height: 120%;
  margin: 0 10px 10px 10px;
  font-size: 16px;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
.home_intro p, .why_login {
  font-family: "Fira Mono";
}
.home_intro_container {
  float: left;
}

/** END Home **/

/** Store **/

.store_wrapper {
  border: 3px solid #58266f; 
  box-sizing: border-box;
  text-align:center;
  width: 190px;
  margin: 0 auto 15px;
  padding: 10px;
}
.store_title {
  font-size: 28px;
  line-height: 26px;
  padding: 0;
  margin: 15px 0;
  text-align: center;
  font-family: "JohnDoe";
}
.store_shirt {
  width: 420px;
  height: 578px;
  margin-top: 5px;
  border: 2px solid #e8d6f1;
}

/** Header **/

#header {
  z-index: 60;
  position: fixed;
  top: 0;
  margin: -5px auto 0;
  width: 100%;
  min-width: 768px;
  height: 54px;
  background: #675032;
  box-shadow: 0 0 15px rgba(90, 100, 117, 0.5);
  border-bottom: 5px solid #7e623d;
}
#page_home {
  width: 800px;
  margin: 20px auto;
}
#header.header_home {
  min-width: 755px;
}
#header span.region_name_page a:hover {
  box-shadow: none;
  text-decoration: none;
  border: none;
  background: transparent;
}
#header .menu_link ul, #header ul.other_regions_list {
  font-size: 12px;
  font-weight: bold;
  line-height: 24px;
  margin-top: 5px;
  float: left;
  padding-left: 0;
  text-align: left;
  display: none;
}
#header .menu_link ul li.first_header_link {
  margin-top: 51px;
}
#header span.menu_link {
  background-color: #ece2d8;
  width: 144px;
  font-size: 24px;
  text-shadow: 1px 1px 5px #CCCCCC;
  line-height: 54px;
  position: absolute;
  text-align: center;
  border: 4px solid #d6c0aa;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
#header span.menu_link:hover {
  cursor: pointer;
}
#header span.menu_link:hover ul {
  display: block;
}
#header .menu_link ul li {
  list-style-type: none;
  font-weight: bold;
  position: relative;
  display: inline;
  padding: 0;
}
#header .menu_link ul {
  font-size: 14px !important;
  font-weight: bold;
  color: #55413F;
  line-height: 18px;
}
#header .menu_link ul li a {
  padding: 5px 6px;
  float: none;
  display: block;
  color: #55413f;
  text-decoration: none;
  vertical-align: middle;
  height: 24.5px;
  width: auto !important;
}
#header .menu_link ul li a:hover {
  color: #441e56;
  background-color: #d6c0aa;
}
#header a img {
    border: 0 none;
    float: right;
    margin: 0 auto;
    padding: 5px 13px 0 0;
    width: 464px;
}
#header .region_logo_dropdown {
    width: 164px;
    height: 55px;
    position: relative;
    float: left;
    margin: 5px 0 0;
    background: #fbfbfb;
    margin-right: 10px;
}
#header li:hover ul, li.over ul, #header li ul li, #header .region_logo_dropdown ul li { 
  display: block;
}
#header li ul, 
#header .region_logo_dropdown ul {
  display: none;
  z-index: 100;
  line-height: 100%;
  list-style: none;
  padding: 8px 5px 5px;
  border-top: none;
  background: #FFFFFF;
  border: 1px solid rgb(102,102,102);
  border-bottom-left-radius: 5px;
  -webkit-border-bottom-left-radius: 5px;
  -moz-border-bottom-left-radius: 5px;
  border-bottom-right-radius: 5px;
  -webkit-border-bottom-right-radius: 5px;
  -moz-border-bottom-right-radius: 5px;
}
#header ul.other_regions_list {
  font-size: 14px;
  position: absolute;
  top: 44px;
  left: 0;
  margin-left: -2px;
  width: 155px;
  max-height: 400px;
  overflow: auto;
  border-top: none;
}
#header ul.other_regions_list a,
#header ul.other_regions_list a:hover {
  text-decoration: none;
  border: none;
  background: transparent;
  padding: 0;
  color: #55413F;
  vertical-align: middle;
  -webkit-box-shadow: none;
  -moz-box-shadow: none;
  box-shadow: none;
}
#header ul.other_regions_list a:hover {
  color: #725755;
}
.other_regions_arrow:after {
  content:"";
    background: url(<%= asset_path 'arrow-sprite-dark.png' %>) no-repeat;
    display: inline-block;
    height: 7px;
    margin-left: 2px;
    width: 7px;
    padding-left:1px;
    vertical-align: middle;
}
.feed_icon:before {
  content:"";
  background: url(<%= asset_path 'feed-icon-10x10.png' %>) no-repeat;
  display: inline-block;
  height: 10px;
  margin-left: 1px;
  width: 10px;
  padding-right:4px;
  vertical-align: middle;
  margin-bottom: 2px;
}
.apps_header, .blog_header, .store_header,
#header .menu_link ul li a {
  font-size: 12px;
  line-height: 11px;
  float: left;
  background-color: #ece2d8;
  text-shadow: 1px 1px 5px #CCCCCC;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
div.pick_a_map .blog_header {
  border-left: 1px solid #d6c0aa;
}
.closest_locations, .geo_header, .login {
  font-size: 15px;
  line-height: 16px;
  padding: 10px 6px;
  margin: 7px 5px;
  border: 4px solid #d6c0aa;
  background-color: #ece2d8;
  text-shadow: 1px 1px 5px #CCCCCC;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
.login {
  float: right;
}
.login a, .login_new a, .login_alert a {
  color: #aa0803;
}
.why_login {
  font-size: 14px;
  color: #725755;
  line-height: 16px;
}
#login .button {
  padding: 6px 6px;
  margin: 7px 5px;
  border: 4px solid #D6C0AA;
  background-color: #ECE2D8;
  text-shadow: 1px 1px 5px #CCC;
  box-sizing: border-box;
  box-shadow: 3px 03px 3px #aaa;
}
#login .button:hover {
  cursor: pointer;
  background: #fbe4dc;
}
#login .reset-password .button,
#login .confirmation .button {
  cursor: pointer;
  width: 260px;
  font-size: 11px;
  font-weight: bold;
}
#login .error {
  font-style: italic;
  color: red;
}
.login_home {
  margin: 2px 5px;
  float: right;
}
.login_alert {
  text-transform: uppercase;
  font-weight: bold;
  background: #fff1f1;
}
div.login_alert {
  padding: 3px;
}
.location_detail .login_alert {
  text-align: center;
}
.geo_header {
  margin: 2px 5px;
  float: right;
}
 #header .menu_link ul li.blog_header_region a, 
 #header .menu_link ul li.store_header_region a {
  border-right: none;
  text-align: center;
  color: #441e56;
}
div.pick_a_map .blog_header:hover, div.pick_a_map .blog_header:active,
div.pick_a_map .apps_header:hover, div.pick_a_map .apps_header:active,
div.pick_a_map .store_header:hover, div.pick_a_map .store_header:active,
div.pick_a_map .geo_header:hover, div.pick_a_map .geo_header:active {
  background-color: #dbc9b6;
}
.darkb.blog_header a:hover,
.darkb.apps_header a:hover, 
.darkb.store_header a:hover,
.darkb.geo_header a:hover {
  text-decoration: none;
}
.region_name, .region_name_page, 
#header span.region_name_page a {
  text-shadow: .05em .05em 0 #e8fd4b, .1em .1em 0 #c719e3;
  -webkit-text-shadow: .05em .05em 0 #e8fd4b, .1em .1em 0 #c719e3;
  color: #58266f;
  font-size: 20px;
  line-height: 22px;
  font-family: 'JohnDoe',Helvetica,Arial,sans-serif;
  text-align: center;
  border-radius: 0;
  display: block;
}
.region_name {
  background-color: #FFFCF8;
  padding: 6px 0;
}
.other_regions {
  display: table !important;
  position: absolute !important;
  height: 50px;
  width: 164px;
  padding-top: 0;
}
span.region_name_page {
  display: table-cell !important;
  text-align: center;
  vertical-align: middle;
}

/** END Header **/

/** Lookup form **/

#operator_section_link, 
#zone_section_link, 
#machine_section_link, 
#city_section_link, 
#location_section_link, 
#type_section_link {
  font-size: 14px;
  background: #FBFBFB;
  border: 1px solid #312f2d;
  color: #642501;
  border-radius: 5px;
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
  box-sizing: border-box;
}
.section {
  padding: 7px 5px 3px;
  margin-top: 30px;
  height: 33px;
  background-color: #d3d1ce;
}
#other_search_options_link:active {
   border-top-color: #484848;
   background: #484848;
}
#other_search_options {
  padding: 0px 0px 4px 6px;
  float: left;
  width: 100%;
}
#other_search_options a {
  padding: 0px 3px 2px;
} 
#other_search_options a:hover {
  background: #853102;
  text-decoration: none;
  border-color: #642501;
  color: #FFFFCC;
}
#locations p {
  margin-top: 20px;
  background-color: #f2eae3;
  padding: 10px;
  border-radius: 5px;
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
  border: 1px solid #9fa7ae;
  font-family: "Fira Mono";
  font-weight: normal;
}
#locations {
  margin-top: 10px;
}
#form {
  position: fixed;
  top: 54px;
  right: 13px;
  width: 630px;
  z-index: 75;
  border: 4px solid #853102;
  border-top: none;
  border-bottom-left-radius: 5px;
  border-bottom-right-radius: 5px;
  padding-top: 4px;
  background-color: #fffef2;
  box-shadow: 0 10px 10px rgba(90, 100, 117, 0.5);
}
#lookup_map, #container {
  width: 100%;
}
#lookup {
  background-color: rgba(197, 168, 135, 0.2);
  border: 2px solid #513D27;
  box-shadow: 0 4px 5px rgba(90, 100, 117, 0.3);
  display: inline;
  float: right;
  padding: 130px 10px 10px 10px;
  width: 600px;
  z-index: 50;
  margin-right: 18px;
  position: relative;
  -webkit-transform: translateZ(0);
}
.search_results_header {
  margin: 20px 0 5px 13px;
  padding: 0 0 5px;
  text-transform: uppercase;
  color: #ebebeb;
  border-bottom: 1px solid white;
}
.location_link {
  cursor: pointer;
  font-size: 12px;
  font-weight: normal;
  padding: 2px 2px 2px;
  margin-top: 5px;
  text-align: center;
  background-color: #f3f2f2;
}
.location_link a:hover {
  text-decoration: underline;
}
#by_type_id, #by_operator_id, #by_location_id, #by_machine_id, #by_zone_id, #by_city_id {
  width: 200px;
  font-size: 16px;
}
.active_section_link {
  background-color: #312f2d !important;
  color: #FFFFCC !important;
}
a.active_section_link:hover {
  background: #1C150B !important;
}

/** Search Results **/

#search_results {
  color: #180e03;
}
.search_result ul {
  border-left: 8px solid #987673;
  padding-left: 0px;
  margin-bottom: 0px;
  border-radius: 5px 0 0 5px;
  -moz-border-radius: 5px 0 0 5px;
  -webkit-border-radius: 5px 0 0 5px;
}
#search_results li {
  list-style-type: none;
  padding-left: 10px;
  padding-right: 5px;
}
.name {
  font-size: 18px;
  line-height: 18px;
  height: 100%;
  color: #433A3A;
  background-color: #FCF3EA;
  padding: 10px 5px;
  border-radius: 0 5px 0 0;
  -moz-border-radius: 0 5px 0 0;
  -webkit-border-radius: 0 5px 0 0;
  font-weight: 700;
}
.address, .address_full {
  font-size: 14px;
  line-height: 16px;
  font-family: "Fira Mono";
}
li.address {
  padding: 2px 0 2px 5px;
  background: #FEFBF8;
}
li.address_full {
  padding: 0 0 0 5px;
  background: #FEFBF8;
  width: 435px;
}
.website {
  font-weight: bold;
  float: left;
  width: 450px;
}
.machine_name {
  font-weight: bold;
  float: left;
  background-color: #e49036;
  color: #ffffff;
  font-size: 22px;
  line-height: 26px;
  height: 100%;
  padding: 0 5px 3px;
  width: 100%;
  margin-left: -10px;
  font-family: "JohnDoe";
}
.machine_name:hover {
  background-color: #e79e55;
}
.machine_names {
  color: #433A3A;
  font-size: 14px;
  padding: 4px 0 25px 10px !important;
  line-height: 18px;
  background: #FEFBF8;
  font-weight: bold;
}
.machine_names:hover { 
  background-color: #fcf3ea;
}
.address_name {
  font-size: 12px;
  line-height: 14px;
}
span.machine_year_man {
  font-size: 16px;
  font-weight: normal;
  font-style: italic;
  color: #433A3A;
}
.machine_lmx {
  color: #180e03;
  font-size: 12px;
  padding: 5px;
  border-left: 5px solid #ae9390;
}
.machine_sub_items {
  width: 400px;
}
.pintips {
  text-align: left;
  float: left;
  font-weight: normal;
  color: #000000;
  font-size: 16px;
  line-height: 18px;
  height: 100%;
  padding: 5px 5px 3px 5px;
  width: 435px;
  box-sizing: border-box;
  clear: both;
  margin-left: 10px;
  border-top: 1px solid #888;
  border-bottom: 1px solid #888;
}
.pintips a {
  font-family: "Fira Mono";
}
.pintips a:after {
  content: "";
  background: url(<%= asset_path 'open-in-new-black.png' %>) no-repeat center center;
  float: right;
  width: 20px;
  height: 20px;
  display: inline;
  background-size: 16px 16px;
  margin: 0 0 0 4px;
}
div.comment_image {
  width: 50px;
  height: 40px;
  float: left;
  margin-bottom: 5px;
}
div.comment_image span.comment_image {
  width: 40px;
}
div.comment_image span.comment_image img {
  float: left;
  cursor: pointer;
  height: 40px;
  margin-top: -15px;
}
div.comment_image span.comment_image:hover:after {
  content: "Leave a comment!";
  position: absolute;
  margin-top: -25px;
  line-height: 14px;
  font-size: 16px;
  margin-left: 10px;
  background: #444444;
  color: #FEFBF8;
  padding: 5px;
  font-weight: bold;
  font-style: italic;
}
.machine_condition_new_line span {
  display: block;
}
.machine_condition_display_lmx span.machine_condition_date {
  color: #999999;
  font-size: 12px;
  margin: 8px 0 2px 0;
  display: inline-block;
}
div.machine_condition_text {
  float: right;
  width: 380px;
}
.machine_condition_display_lmx .machine_condition_lmx span.machine_condition_date {
  margin-bottom: 8px;
}
.machine_condition_new_line span.machine_condition_date {
  margin: 8px 0 2px;
  display: inline-block;
  color: #999999;
  font-size: 12px;
}
form.remove_machine_condition {
  float: right;
  padding-right: 5px;
}
.machine_condition_new_line,
.machine_condition_display_lmx {
  color: #333333;
  font-size: 14px;
  line-height: 16px;
}
.machine_condition_new_line {
  border-bottom: 1px solid #d0b89f;
  margin-bottom: 8px;
}
.machine_condition_display_lmx {
  margin: 5px 0 5px 10px;
  padding: 2px;
  box-sizing: border-box;
  width: 435px;
}
.machine_condition_display_lmx .edit_mode {
  width: 400px;
  max-width: 400px;
  min-width: 400px;
  font-family: "Fira Mono";
}
.machine_condition_display_lmx .condition_button {
  height: 20px;
  line-height: 20px;
  box-shadow: none;
  background: none;
  text-transform: uppercase;
  font-size: 15px;
  border: none;
  width: auto;
