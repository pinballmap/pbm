/*
 *= require jquery-ui
 */

@import url(http://fonts.googleapis.com/css?family=Open+Sans:400,600);
@import url(http://fonts.googleapis.com/css?family=Montserrat:400,700);

/** Core **/

* {
  outline: none;
}
body {
  margin: 0 auto;
  padding: 0px;
  background-image: url(<%= asset_path 'PPM-SplashTile-BW-Inverted.png' %>);
  background-color: #FFFFFF;
  background-size: 4px auto;
  color: #2e2322;
  font-family: 'Open Sans',Helvetica,Arial,sans-serif;
  -webkit-font-smoothing: antialiased;
}
p {
  font-size: 16px;
  line-height: 20px;
}
img {
  border: 0;
}
h1 {
  font-size: 36px;
}
a {
  color: #8A3CAE;
  text-decoration: none;
  border: 0;
}
a:hover {
  text-decoration: underline;
  color: #58266F;
}
input, textarea {
  -webkit-appearance: none;
  -webkit-border-radius: 0;
  -moz-border-radius: 0;
  border-radius: 0;
  -webkit-box-sizing: border-box; /* Safari/Chrome, other WebKit */
  -moz-box-sizing: border-box;    /* Firefox, other Gecko */
  box-sizing: border-box;         /* Opera/IE 8+ */
}
ul {
  font-size: 16px;
}
li {
  padding: 2px 0 6px 4px;
}
li a:hover {
  text-decoration: underline;
}
.subpage_text {
  font-size: 14px;
  letter-spacing: .3px;
}
.pink {
  color: #ffcc00;
  font-size: 14px;
  font-weight: bold;
}

/* Text */

.intro {
  font-size: 16px;
}
.dark {
  color: #706b65;
}
.darkb {
  color: #706b65;
  font-weight: bold;
  text-shadow: 0.1px 0.1px 0.1px white;
}
.hello {
  font-size: 28px;
  line-height: 36px;
  text-shadow: 1px 1px 0 rgba(255, 255, 255, 0.8);
  margin: 0;
}

/** END Core **/

/** Login **/

#login {
  background: none repeat scroll 0 0 rgba(253, 252, 254, 0.7);
  border: 2px solid #E3D6C5;
  margin: 70px auto 0;
  padding: 0 20px 20px;
  width: 270px;
}
#login label.password,
#login label.email {
  display: block;
}
#login label.boolean {
  font-size: 12px;
}
#login input.boolean {
  height: 16px;
}
form.new_user {
  font-size: 16px;
}
#login input {
  height: 25px;
}
#login .form-actions {
  line-height: 40px;
}
#login abbr {
  border: none;
}

/** Home **/

ul.twitter {
  list-style-type: none;
  padding: 0;
  margin-bottom: 0;
}
.twitter li {
  padding: 5px 0;
  border-bottom: 1px solid #DECEB9;
  font-family: 'Open Sans';
  color: #452809;
  font-size: 15px;
}
.twitter li:nth-child(5) { 
  border: none;
}
p.notice {
  font-size: 18px;
  text-align: center;
  margin: -20px auto 10px;
}
.thanks, p.alert {
  font-size: 28px;
  text-align: center;
  margin: 50px auto 0; 
  text-shadow: 0.1em 0.1em 0.2em black;
  color: #FFFFCC;
  background: #e3e1e0;
  border-bottom: 1px solid #c3c0bc;
  border-top: 1px solid #c3c0bc;
  padding: 10px;
}
div.pick_a_map {
  font-size: 28px;
  line-height: 42px;
  text-align: center;
  padding-top: 5px;
}
span.pick_a_map, .store_title {
  color: #58266f;
  margin: 0;
  text-shadow: 1px 1px 5px #CCCCCC;
}
div.dont_pick_a_map {
  display: none;
}
.home_intro {
  padding: 3px 6px 6px;
  background-color: #fdfcfe;
  border: 5px solid #efe2f5;
  width: 300px;
  line-height: 120%;
  margin: 0 10px 10px 10px;
  font-size: 16px;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
.home_intro_container {
  float: left;
}

/** END Home **/

/** Store **/

.store_wrapper {
  border: 3px solid #58266f; 
  box-sizing: border-box;
  text-align:center;
  width: 190px;
  margin: 0 auto 15px;
  padding: 10px;
}
.store_title {
  font-size: 28px;
  line-height: 26px;
  padding: 0;
  margin: 15px 0;
  text-align: center;
}
.store_shirt {
  width: 420px;
  height: 578px;
  margin-top: 5px;
  border: 2px solid #e8d6f1;
}

/** Header **/

#header {
  z-index: 60;
  position: fixed;
  top: 0;
  margin: -5px auto 0;
  width: 100%;
  min-width: 768px;
  height: 54px;
  background: #FFFFFF;
  box-shadow: 0 0 15px rgba(90, 100, 117, 0.5);
  border-bottom: 5px solid #D8C6AE;
}
#page_home {
  width: 800px;
  margin: 20px auto;
}
#header.header_home {
  min-width: 755px;
}
#header span.region_name_page a:hover {
  box-shadow: none;
  text-decoration: none;
  border: none;
  background: transparent;
}
#header ul.region_links, #header ul.other_regions_list, #header ul.other_links_dropdown {
  font-size: 12px;
  font-weight: bold;
  line-height: 22px;
  margin-top: 5px;
  margin-left: 150px;
  padding-left: 0;
  text-align: left;
  width: 285px;
}
#header ul.region_links li {
  list-style-type: none;
  font-weight: bold;
  position: relative;
  display: inline;
}
#header ul.region_links li a {
  padding: 2px 4px;
  color: #55413f;
  text-decoration: none;
  vertical-align: middle;
}
#header ul.region_links li a:hover {
  color: #8A3CAE;
}
#header a img {
    border: 0 none;
    float: right;
    margin: 0 auto;
    padding: 5px 13px 0 0;
    width: 464px;
}
#header ul.region_links li:nth-child(3):after { 
  content: '\a'; white-space:pre; 
}
#header .region_logo_dropdown {
    width: 160px;
    height: 50px;
    position: relative;
    float: left;
    margin: 5px 0 0;
}
#header li:hover ul, li.over ul, #header li ul li, #header .region_logo_dropdown ul li { 
  display: block; 
}
#header li ul, 
#header .region_logo_dropdown ul {
  display: none;
  z-index: 100;
  line-height: 100%;
  list-style: none;
  padding: 8px 5px 5px;
  border-top: none;
  background: #FFFFFF;
  border: 1px solid rgb(102,102,102);
  border-bottom-left-radius: 5px;
  -webkit-border-bottom-left-radius: 5px;
  -moz-border-bottom-left-radius: 5px;
  border-bottom-right-radius: 5px;
  -webkit-border-bottom-right-radius: 5px;
  -moz-border-bottom-right-radius: 5px;
}
#header ul.other_regions_list {
  font-size: 14px;
  position: absolute;
  top: 44px;
  left: 0;
  margin-left: -2px;
  width: 155px;
  max-height: 400px;
  overflow: auto;
  border-top: none;
}
#header ul.other_links_dropdown {
  position: absolute;
  right: 3px;
  top: 15px;
  border:none;
  background-color: #FFFFFF;
  width: 93px;
}
#header ul.other_regions_list a,
#header ul.other_regions_list a:hover {
  text-decoration: none;
  border: none;
  background: transparent;
  padding: 0;
  color: #55413F;
  vertical-align: middle;
  -webkit-box-shadow: none;
  -moz-box-shadow: none;
  box-shadow: none;
}
#header ul.other_regions_list a:hover {
  color: #725755;
}
.other_regions_arrow:after {
  content:"";
    background: url(<%= asset_path 'arrow-sprite-dark.png' %>) no-repeat;
    display: inline-block;
    height: 7px;
    margin-left: 2px;
    width: 7px;
    padding-left:1px;
    vertical-align: middle;
}
.feed_icon:before {
  content:"";
  background: url(<%= asset_path 'feed-icon-10x10.png' %>) no-repeat;
  display: inline-block;
  height: 10px;
  margin-left: 1px;
  width: 10px;
  padding-right:4px;
  vertical-align: middle;
  margin-bottom: 2px;
}
.apps_header, .blog_header, .store_header {
  font-size: 15px;
  line-height: 36px;
  float: left;
  width: 80px;
  padding: 5px 6px 8px;
  background-color: #f6effa;
  text-shadow: 1px 1px 5px #CCCCCC;
  border-left: 1px solid #efe2f6;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
.store_header {
  border-right: 1px solid #efe2f6;;
}
.blog_header_region, .store_header_region {
  border-right: none;
  float: right;
  padding: 10px 6px 8px;
  text-align: center;
}
.blog_header:hover,
.apps_header:hover,  
.store_header:hover {
  background-color: #efe2f6;
}
.darkb.blog_header a:hover,
.darkb.apps_header a:hover, 
.darkb.store_header a:hover {
  text-decoration: none;
}
.region_name, .region_name_page, 
#header span.region_name_page a {
  text-shadow: .05em .05em 0 #e8fd4b, .1em .1em 0 #c719e3;
  -webkit-text-shadow: .05em .05em 0 #e8fd4b, .1em .1em 0 #c719e3;
  color: #58266f;
  font-size: 20px;
  line-height: 22px;
  font-family: 'Montserrat',Helvetica,Arial,sans-serif;
  text-align: center;
  border-radius: 0;
  display: block;
}
.region_name {
  background-color: #FFFCF8;
  padding: 10px 0;
}
.other_regions {
  display: table !important;
  position: absolute !important;
  height: 50px;
  width: 150px;
  padding-top: 0;
}
span.region_name_page {
  display: table-cell !important;
  text-align: center;
  vertical-align: middle;
}

/** END Header **/

/** Lookup form **/

#operator_section_link, 
#zone_section_link, 
#machine_section_link, 
#city_section_link, 
#location_section_link, 
#type_section_link {
  font-size: 15px;
  background: #FBFBFB;
  border-top: #ffeedd;
  color: black;
  border-radius: 5px;
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
}
.section {
  padding: 2px 5px 8px;
  width: 446px;   
  height: 25px;
  margin-top: 27px;
  border: 2px solid #513D27;
  border-top: none;
  background-color: #f2eae3;
  box-shadow: 0 10px 10px rgba(90, 100, 117, 0.5);
}
#other_search_options_link:active {
   border-top-color: #484848;
   background: #484848;
}
#other_search_options {
  background: none repeat scroll 0 0 #FBFBFB;
  padding: 0px 0px 4px 15px;
  width: 445px;
  float: left;
}
#other_search_options a {
  padding: 0px 6px 2px;
} 
#other_search_options a:hover {
  background: #6e5335;
  border-top-color: #70706a;
  text-decoration: none;
  color: #FFFFCC;
}
#locations p {
  margin-top: 20px;
  background-color: #f2eae3;
  padding: 10px;
  border-radius: 5px;
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
  border: 1px solid #9fa7ae;
}
#locations {
  margin-top: 10px;
}
#form {
  position: fixed;
  top: 49px;
  right: 13px;
  width: 460px;
  height: 25px;
  z-index: 75;
  border: 2px solid #513D27;
  padding-top: 4px;
  padding-bottom: 2px;
  background-color: white;
}
#lookup_map, #container {
  width: 100%;
  /*margin: -10px auto 20px;*/
}
#lookup {
  background-color: rgba(197, 168, 135, 0.2);
  border: 2px solid #513D27;
  box-shadow: 0 4px 5px rgba(90, 100, 117, 0.3);
  display: inline;
  float: right;
  padding: 130px 10px 60px 10px;
  width: 430px;
  z-index: 50;
  margin-right: 18px;
  position: relative;
  -webkit-transform: translateZ(0);
}
.search_results_header {
  margin: 20px 0 5px 13px;
  padding: 0 0 5px;
  text-transform: uppercase;
  color: #ebebeb;
  border-bottom: 1px solid white;
}
.location_link {
  color: #999999;
  cursor: pointer;
  font-size: 11px;
  font-weight: normal;
  border: 1px solid #494949;
  border-bottom: none;
  padding: 1px 2px 2px;
  margin-left: 275px;
  margin-top: 5px;
  height: 10px;
  text-align: center;
  background-color: #f3f2f2;
}
.location_link:hover {
  color: #cccccc;
  background-color: #e3e1e0;
}
#by_type_id, #by_operator_id, #by_location_id, #by_machine_id, #by_zone_id, #by_city_id {
  margin-top: 5px;
  margin-bottom: 3px;
  width: 145px;
  font-size: 14px;
}
.active_section_link {
  background-color: #A6A2A6 !important;
  color: #FFFFCC !important;
}
a.active_section_link:hover {
  background: #1C150B !important;
}

/** Search Results **/

#search_results {
  color: #180e03;
}
.search_result ul {
  border-left: 8px solid #987673;
  padding-left: 0px;
  margin-bottom: 0px;
  border-radius: 5px 0 0 5px;
  -moz-border-radius: 5px 0 0 5px;
  -webkit-border-radius: 5px 0 0 5px;
}
#search_results li {
  list-style-type: none;
  font-weight: bold;
  padding-left: 10px;
}
.name {
  font-size: 16px;
  line-height: 16px;
  height: 100%;
  color: #433A3A;
  background-color: #e7dfdf;
  padding: 10px 5px 5px;
  margin-bottom:3px;
  border-radius: 0 5px 0 0;
  -moz-border-radius: 0 5px 0 0;
  -webkit-border-radius: 0 5px 0 0;
}
.address {
  font-size: 12px;
  line-height: 14px;
}
li.address {
  padding: 2px 0 2px 5px;
  font-weight: normal !important;
}
.machine_name { /* comment about where this is used */
  font-weight: bold;
  float: left;
  background-color: #EBAF73;
  color: #282222;
  font-size: 16px;
  line-height: 20px;
  height: 100%;
  padding: 0 5px 3px;
  width: 78%;
}
.machine_name:hover {
  background-color: #e79e55;
}
.machine_names {
  color: #433A3A;
  font-size: 14px;
  padding: 4px 0 8px 10px !important;
  line-height: 18px;
}
.address_name {
  font-size: 12px;
  line-height: 14px;
}
span.machine_year_man {
  font-weight: normal;
  color: #433A3A;
}
.machine_lmx {
  color: #180e03;
  font-size: 12px;
  padding: 5px;
  border-left: 5px solid #ae9390;
}
.machine_condition_display_lmx {
  color: #888888;
  font-size: 12px;
  font-style: italic;
  margin: 5px 0 5px 10px;
  padding: 2px;
  line-height: 14px;
  width: 281px;
}
.machine_condition_display_lmx .edit_mode {
  width: 261px;
  max-width: 261px;
}
.machine_condition_display_lmx:hover {
  background-color: #dbd9d7;
}
.machine_condition_display_lmx span {
  white-space: pre-wrap;
}
.machine_condition_edit_lmx {
  width: 275px;
}
.machineconditions_container_lmx {
  margin-left: 10px;
}
.machine_condition_new_line {
  display:block;
  font-size:10px;
  padding: 4px 0;
}
.high_score { 
  margin-left: 10px;  
}
.high_score > .sub_nav_item {
  width: 283px;
  clear: both;
}
#add_machine_by_id {
  margin-bottom: 5px;
  margin-left: 13px;
  width: 306px;
}
#add_machine_by_name {
  width: 304px;
  margin-left: 12px;
}
.add_picture_location span.info, .show_machines_location span.info, .add_machine_location span.info,
.add_picture_location, .new_location_picture_xref, 
.add_machine_location, .show_machines_location {
  display: block;
  font-size: 11px;
  line-height: 14px;
  margin-left: 13px;
  width: 380px;
  padding: 5px 0px;
}
.add_picture_location span.info, .show_machines_location span.info, .add_machine_location span.info {
  width: 360px;
  padding: 0 0 10px 0;
}
.add_picture_location {
  background-color: #DBE5FC;
}
.add_machine_location {
  background-color: #eaf1ed;
}
.show_machines_location {
  background-color: #fcf3ea;
}
.button_to {
  float:right;
}
.location_detail input.remove_machine_button {
  margin: 0;
  background-color:#fcf3ea;
  border: 1px solid #999999;
  color: #999999; 
  cursor: pointer;
  padding: 0 2px 2px;
  font-size: 12px;
  line-height: 20px;
  height: 23px;
  width: 60px;
}
.location_detail input.remove_machine_button:hover {
  border: 1px solid #3b3835;
  color: black;
  background-color: #f4d1af;
}
.machine_title {
  margin-top: 20px;
}
br.clear {
  margin-top: 10px;
}
.sub_nav_item img {
  padding: 2px 0;
  background: #DDDDDD;
}
.sub_nav_item {
  height: 18px;
  line-height: 18px;
  padding-left: 2px;
  cursor: pointer;
  font-size: 14px;
  font-weight: normal;
  color: #f9f9f9;
  cursor: pointer;
  width: 405px;
}
.sub_nav_item:hover {
  color: #ffffff;
}
.arrow {
  float: right;
  height: 14px;
  width: 14px;
}
.arrow:hover {
  border-color: #bbb7b3;
}
.sub_nav_location {
  font-size: 14px;
  line-height: 16px;
  font-weight: normal;
  width: 405px;
  color: #f9f9f9;
  cursor: pointer;
  height: 100%;
  padding: 2px 0 1px 2px;
}
.sub_nav_location li.address {
  color: #666666;
}
.search_result {
  height: 100%;
  width: 430px;
  background-color: #FBFBFB;
  border-radius: 5px;
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
  margin-bottom: 12px;
}
.show_location_detail_location ul:hover {
  background-color: #f3f3f3;
  border-radius: 5px;
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
}
#form .search {
  float: right;
  font-size: 14px;
  height: 22px;
  line-height: 15px;
  margin-top: 4px;
  margin-right: 0;
  padding: 0 2px;
  text-transform: uppercase;
}
#form select {
  border: 1px solid #b0b0b0;
  background-color: #e9e7e9;
}
input#add {
  width: 40px;
}
.add_picture_location input.submit_picture {
  width: auto;
}
.new_location_picture_xref label {
  line-height: 24px;
}
.search, 
.submit_button, 
.save_button, 
.cancel_button, 
#add_score, 
#add, 
#location_picture_xref_submit, 
.new_location_picture_xref input.submit_picture,
#login .button {
  font-style: normal;
  color: #303030;
  background-color: #F6EFFA;
  border: 2px solid #D0B89F;
  text-shadow: 1px 1px 0px #fcfcfc;
  box-shadow: 2px 2px 3px #2a2b27;
  -moz-box-shadow: 2px 2px 3px #2a2b27;
  -webkit-box-shadow: 2px 2px 3px #2a2b27;
  padding: 0 4px;
  cursor: pointer;
  margin-right: 4px;
  font-size: 14px;
}
.search:active, 
.search:focus, 
.submit_button:active, 
.save_button:active, 
.cancel_button:active, 
#add_score:active, 
#add:active, 
#location_picture_xref_submit:active,
#login .button:active {
  box-shadow: 1px 1px 1px #2a2b27;
  -moz-box-shadow: 1px 1px 1px #2a2b27;
  -webkit-box-shadow: 1px 1px 1px #2a2b27;
  transform: translate(1px, 1px);
  background: #f8f8a1;
}
.save_button {
  float: left;
}
input:focus { /* this is in need of an update */
  box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075) inset, 0 0 8px rgba(249, 252, 159, 0.6);
  -moz-box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075) inset, 0 0 8px rgba(249, 252, 159, 0.6);
  -webkit-box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075) inset, 0 0 8px rgba(249, 252, 159, 0.6);
}
input.search:hover, 
input#add:hover, 
input#location_picture_xref_submit:hover, 
input.submit_button:hover,
.save_button:hover, 
.cancel_button:hover, 
#add_score:hover {
  box-shadow: 2px 2px 4px #2a2b27;
  -moz-box-shadow: 2px 2px 4px #2a2b27;
  -webkit-box-shadow: 2px 2px 4px #2a2b27;
  background: #f8f8a1;
}

#location_maker input.text, 
#location_maker textarea,
#contact_maker input.text, 
#contact_maker textarea, 
.location_detail input, 
#form input.lookup_search_input, 
.edit_mode,
#new_user input[type="email"],
#new_user input[type="password"] {
  background-color: #fcfcf3;
  border: 2px solid #D0B89F;
  border-radius: 0;
  -moz-border-radius: 0;
  -webkit-border-radius: 0;
  margin: 4px 4px;
  width: 260px;
}
#form input.lookup_search_input {
  width: 200px;
  height: 22px;
  font-size: 14px;
}
.location_detail input {
  width: 80px;
}
input#score, input#initials {
  width: 80%;
}
.score_container_lmx {
  margin-left: 10px;
}
.add_scores_form, .show_scores_lmx, .show_conditions_lmx {
  padding: 5px 0 5px 10px;
  background-color: #FEFBF8;
