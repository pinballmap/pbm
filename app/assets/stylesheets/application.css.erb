  font-weight: 400;
}
#intro .darkb {
  font-family: "Palatino","Georgia","Times New Roman", Times,serif;
  font-weight: 700;
  font-size: 16px;
}
.to_begin {
  margin-top: 10px;
  font-family: 'Freight-MedSC',Helvetica,Arial,sans-serif;
  font-weight: bold;
}
@-webkit-keyframes leftToRight {
    0% {
        margin-left: 10px;
        -webkit-transform: scale(1);
    }
    50% {
        margin-left: 150px;
        -webkit-transform: scale(2);
    }
    100% {
        margin-left: 10px; 
        -webkit-transform: scale(1);
    }
}
@-moz-keyframes leftToRight {
    0% {
        margin-left: 10px; 
        -moz-transform: scale(1);
    }
    50% {
        margin-left: 150px;
        -moz-transform: scale(2);
    }
    100% {
        margin-left: 10px; 
        -moz-transform: scale(1);
    }
}
@keyframes leftToRight {
    0% {
        margin-left: 10px;
        transform: scale(1);
    }
    50% {
        margin-left: 150px;
        transform: scale(2);
    }
    100% {
        margin-left: 10px; 
        transform: scale(1);
    }
}
@-o-keyframes leftToRight {
    0% {
        margin-left: 10px; 
    }
    50% {
        margin-left: 150px;
    }
    100% {
        margin-left: 10px; 
    }
}
.to_begin .search_arrow {
    -webkit-animation-duration: 5s;
    -webkit-animation-name: leftToRight;
    -webkit-animation-delay: 1s;
    -webkit-animation-timing-function: ease-in-out;

    -moz-animation-duration: 5s;
    -moz-animation-name: leftToRight;
    -moz-animation-delay: 1s;
    -moz-animation-timing-function: ease-in-out;

    -o-animation-duration: 5s;
    -o-animation-name: leftToRight;
    -o-animation-delay: 1s;
    -o-animation-timing-function: ease-in-out;

    animation-duration: 5s;
    animation-name: leftToRight;
    animation-delay: 1s;
    animation-timing-function: ease-in-out;
}
@-webkit-keyframes colorChange {
    0% {
        color: #2e2322;
    }
    50% {
        color: #303030;
        text-shadow: 1px 1px 5px #CCCCCC;
        background-color: #d3ecff;
    }
    100% {
        color: #2e2322;
    }
}
@-moz-keyframes colorChange {
    0% {
        color: #2e2322;
    }
    50% {
        color: #303030;
        text-shadow: 1px 1px 5px #CCCCCC;
        background-color: #d3ecff;
    }
    100% {
        color: #2e2322;
    }
}
@keyframes colorChange {
    0% {
        color: #2e2322;
    }
    50% {
        color: #303030;
        text-shadow: 1px 1px 5px #CCCCCC;
        background-color: #d3ecff;
    }
    100% {
        color: #2e2322;
    }
}
@-o-keyframes colorChange {
    0% {
        color: #2e2322;
    }
    50% {
        color: #303030;
        text-shadow: 1px 1px 5px #CCCCCC;
        background-color: #d3ecff;
    }
    100% {
        color: #2e2322;
    }
}
span.begin {
    padding: 10px;
    background-color: #EFEFEF;

    -webkit-animation-duration: 5s;
    -webkit-animation-name: colorChange;
    -webkit-animation-delay: 1s;
    -webkit-animation-timing-function: ease-in-out;
    -webkit-animation-iteration-count: 1;

    -moz-animation-duration: 5s;
    -moz-animation-name: colorChange;
    -moz-animation-delay: 1s;
    -moz-animation-timing-function: ease-in-out;
    -moz-animation-iteration-count: 1;

    -o-animation-duration: 5s;
    -o-animation-name: colorChange;
    -o-animation-delay: 1s;
    -o-animation-timing-function: ease-in-out;
    -o-animation-iteration-count: 1;

    animation-duration: 5s;
    animation-name: colorChange;
    animation-delay: 1s;
    animation-timing-function: ease-in-out;
    animation-iteration-count: 1;
}
