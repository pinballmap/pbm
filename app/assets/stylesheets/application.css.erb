/*
 *= require jquery-ui
 */

/** Core **/

@font-face {
  font-family: 'Freight-MedSC';
  src: url(<%= asset_path "freight-sans-medium-sc-opentype.otf" %>);
}
* {
  outline: none;  
}
body {
  margin: 0 auto;
  padding: 0px;
  background-color: #FFFFFF;
  background-size: 4px auto;
  color: #171717;
  font-family: "Freight-MedSC",Helvetica,Arial,sans-serif;
  -webkit-font-smoothing: antialiased;
}
p {
  font-size: 16px;
  line-height: 22px;
}
img {
  border: 0;
}
h1 {
  font-size: 36px;
}
a {
  color: #383838;
  text-decoration: underline;
  border: 0;
}
a:hover {
  text-decoration: underline;
  color: #F53240;
}
input, textarea {
  -webkit-appearance: none;
  -webkit-border-radius: 0;
  -moz-border-radius: 0;
  border-radius: 0;
  -webkit-box-sizing: border-box; /* Safari/Chrome, other WebKit */
  -moz-box-sizing: border-box;    /* Firefox, other Gecko */
  box-sizing: border-box;         /* Opera/IE 8+ */
  font-family: "Palatino","Georgia","Times New Roman", Times,serif;
}
select {
  font-family: Helvetica,Arial,sans-serif;
}
ul {
  font-size: 16px;
}
li {
  padding: 2px 0 6px 4px;
}
li a:hover {
  text-decoration: underline;
}
ul li li {
  list-style-type: decimal;
}
.notbold {
  font-weight: normal;
}
.subpage_text {
  font-size: 14px;
  letter-spacing: .3px;
}
.pink {
  color: #ffcc00;
  font-size: 14px;
  font-weight: bold;
}

/* Text */

.intro {
  font-size: 18px;
}
.dark {
  color: #0b252b;
}
.darkb, .darkbl {
  color: #F53240;
  text-shadow: 0.1px 0.1px 0.1px white;
  font-family: "Freight-MedSC",Helvetica,Arial,sans-serif;
  font-size: 18px;
  font-weight: bold;
}
.darkbl {
  color: #303030;
}
span.stats {
  background: #EFEFEF;
  font-size: 22px; 
  line-height: 18px;
  padding: 2px 2px 6px 2px;
  float: right;
}
.italic {
  font-style: italic;
}
.bold {
  font-weight: bold;
}
.hello {
  font-size: 28px;
  line-height: 36px;
  text-shadow: 1px 1px 0 rgba(255, 255, 255, 0.8);
  margin: 0;
  font-weight: 700;
  color: #F53240;
}

/** END Core **/

/** Login **/

#login {
  background: none repeat scroll 0 0 rgba(253, 252, 254, 1);
  border: 2px solid #EFEFEF;
  margin: 10px auto 0;
  padding: 0 20px 20px;
  width: 270px;
}
#login label.password,
#login label.email,
#login label.string {
  display: block;
  font-size: 20px;
}
#login label.boolean {
  font-size: 12px;
}
#login input.boolean {
  height: 16px;
  width: 16px;
  border: 1px solid #303030;
  margin: 0 5px 0 0;
}
form.new_user, form.edit_user {
  font-size: 16px;
}
form.new_user .input, form.edit_user .input {
  margin-top: 10px;
}
form.new_user .form-actions, form.edit_user .form-actions {
  margin-top: 10px;
}
#login abbr {
  border: none;
}
.login_why_outer {
  width: 550px;
  margin: 0 auto;
}
.login_why_inner {
  width: 210px;
  background-color: #EFEFEF;
  border: 2px solid #d3ecff;
  float: left;
  padding: 10px;
  margin-top: 10px;
}
.login_why_inner p.why_login {
  font-size: 18px;
  font-weight: bold;
}
.login_new_div {
  float: right;
  margin: 0;
}
.login_slash {
  color: #303030;
}

/** Stats **/

p.stats {
  line-height: 22px;
  border-bottom: 1px solid #d3ecff;
  padding-bottom: 15px;
}

/** Home **/

ul.twitter {
  list-style-type: none;
  padding: 0;
  margin-bottom: 0;
}
.twitter li {
  padding: 5px 0;
  border-bottom: 1px solid #DECEB9;
  font-family: "Palatino","Georgia","Times New Roman", Times,serif;
  color: #452809;
  font-size: 15px;
}
.twitter li:nth-child(5) { 
  border: none;
}
.home_intro .twitter_box {
  font-family: "Freight-MedSC",Helvetica,Arial,sans-serif;
}
p.notice {
  color: #F53240;
  font-size: 18px;
  text-align: center;
  margin: -10px auto 10px;
  width: 100%;
}
.thanks, p.alert {
  font-size: 28px;
  line-height: 36px;
  text-align: center;
  margin: 50px auto 0; 
  text-shadow: 0.1em 0.1em 0.2em black;
  color: #FFFFCC;
  background: #d3ecff;
  border-bottom: 1px solid #c3c0bc;
  border-top: 1px solid #c3c0bc;
  padding: 10px;
}
#flash_error {
  font-size: 16px;
  margin-top: 120px;
  position: fixed;
  background: pink none repeat scroll 0% 0%;
  padding: 10px;
  right: 20px;
  z-index: 4000;
  border: 3px solid black;
}
#flash_recaptcha_error {
  display: none;
}
#flash-message.flash_home {
  width: 100%;
  float: left;
}
#flash-message.flash_region {
  width: 100%;
  float: none;
}
#flash-message.flash_region #flash_notice,
#flash-message.flash_region #flash_alert {
  position: absolute;
  top: 0;
  margin: 0;
  width: 100%;
  max-width: 100%;
}
#flash-message #flash_notice,
#flash-message #flash_alert,
#flash-message.flash_region #flash_notice,
#flash-message.flash_region #flash_alert {
  color: #F53240;
  font-size: 18px;
  text-align: center;
  margin: 0px auto 10px;
  padding: 5px 0;
  width: 100%;
  background: #fdf0eb;
  position: absolute;
  z-index: 150;
}
.home_intro {
  padding: 3px 6px 6px;
  background-color: #EFEFEF;
  border: 5px solid #EFEFEF;
  width: 300px;
  line-height: 120%;
  margin: 0 10px 10px 10px;
  font-size: 18px;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
.home_intro p, span.why_login {
  font-family: "Palatino","Georgia","Times New Roman", Times,serif;
}
.home_intro_container {
  float: left;
}

/** END Home **/

/** Store **/

.store_wrapper {
  border: 3px solid #F53240; 
  -webkit-box-sizing: border-box; 
  -moz-box-sizing: border-box;    
  box-sizing: border-box;
  text-align:center;
  width: 190px;
  margin: 0 auto 15px;
  padding: 10px;
}
.store_title, .question.darkb, .donate.darkb,
.suggest p.darkb, .high p.darkb {
  font-size: 28px;
  line-height: 26px;
  padding: 0;
  margin: 15px 0;
  text-align: center;
  font-family: "Freight-MedSC",Helvetica,Arial,sans-serif;
  font-weight: bold;
}
.store_shirt {
  width: 420px;
  height: 578px;
  margin-top: 2px;
  border: 2px solid #d3ecff;
}

/** Header **/

#header {
  z-index: 60;
  position: fixed;
  top: 0;
  margin: -5px auto 0;
  width: 100%;
  min-width: 768px;
  height: 54px;
  background: #d3ecff;
  border-bottom: 3px solid #d3ecff;
}
#header a {
  text-decoration: none;
}
#page_home {
  width: 800px;
  margin: 20px auto;
}
#page {
  height: 64px;
}
#header.header_home {
  min-width: 755px;
}
#header span.region_name_page a:hover {
  box-shadow: none;
  text-decoration: none;
  border: none;
  background: transparent;
}
#header .menu_link ul, #header ul.other_regions_list {
  font-size: 12px;
  line-height: 24px;
  margin-top: -6px;
  padding-left: 0;
  text-align: left;
  display: none;
  box-shadow: 5px 7px 10px rgba(90, 100, 117, 0.5);
}
#header .menu_link ul li.first_header_link a {
  padding-top: 10px;
}
#header .menu_link ul li.first_global_link:before {
  border-bottom: 1px solid #A9A9A9;
  content: "";
  position: absolute;
  height: 1px;
  width: 145px;
  margin: 10px 10px 0;
}
#header .menu_link ul li.last_header_link a {
  padding-bottom: 10px;
}
#header span.menu_link {
  width: 50px;
  font-size: 32px;
  line-height: 63px;
  text-align: center;
  float: left;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  height: 54px;
}
#header span.menu_link:hover {
  cursor: pointer;
}
#header span.menu_link:hover ul {
  display: block;
}
#header .menu_link ul li {
  list-style-type: none;
  position: relative;
  display: inline;
  padding: 0;
}
#header .menu_link ul {
  font-size: 14px !important;
  color: #55413F;
  width: 165px;
}
#header .menu_link ul li a {
  text-align: left !important;
  padding: 4px 15px;
  float: none;
  display: block;
  text-decoration: none;
  vertical-align: middle;
  width: auto !important;
  font-size: 16px;
  line-height: 16px;
  font-weight: bold;
  background-color: #EFEFEF;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
#header .menu_link ul li a:hover {
  color: #303030;
  background-color: #d3ecff;
}
#header .menu_home ul li a {
  background-color: #D3ECFF;
}
#header .menu_home ul li a:hover {
  background-color: #EFEFEF;
}
#header a img {
  border: 0 none;
  float: right;
  margin: 0 auto;
  padding: 5px 13px 0 0;
  width: 464px;
}
#header .region_logo_dropdown {
  width: 164px;
  height: 49px;
  position: relative;
  float: right;
  margin: 5px 0 0;
}
#header li:hover ul, li.over ul, #header li ul li, #header .region_logo_dropdown ul li { 
  display: block;
}
#header li ul, 
#header .region_logo_dropdown ul {
  display: none;
  z-index: 100;
  line-height: 100%;
  list-style: none;
  padding: 8px 5px 5px;
  border-top: none;
  background: #EFEFEF;
  border: 1px solid rgb(102,102,102);
  border-bottom-left-radius: 5px;
  -webkit-border-bottom-left-radius: 5px;
  -moz-border-bottom-left-radius: 5px;
  border-bottom-right-radius: 5px;
  -webkit-border-bottom-right-radius: 5px;
  -moz-border-bottom-right-radius: 5px;
}
#header ul.other_regions_list {
  font-size: 16px;
  position: absolute;
  top: 46px;
  left: 0;
  margin-top: 4px;
  width: 160px;
  max-height: 400px;
  overflow: auto;
  border-top: none;
  z-index: 100;
  font-weight: bold;
}
#header ul.other_regions_list a,
#header ul.other_regions_list a:hover {
  text-decoration: none;
  border: none;
  background: transparent;
  padding: 0;
  color: #303030;
  vertical-align: middle;
  -webkit-box-shadow: none;
  -moz-box-shadow: none;
  box-shadow: none;
}
#header ul.other_regions_list a:hover {
  color: #F53240;
}
.other_regions_arrow:after {
  content:"";
    background: url(<%= asset_path 'arrow-sprite-dark.png' %>) no-repeat;
    display: inline-block;
    height: 7px;
    margin-left: 2px;
    width: 7px;
    padding-left:1px;
    vertical-align: middle;
}
.feed_icon:before {
  content:"";
  background: url(<%= asset_path 'feed-icon-10x10.png' %>) no-repeat;
  display: inline-block;
  height: 10px;
  margin-left: 1px;
  width: 10px;
  padding-right:4px;
  vertical-align: middle;
  margin-bottom: 2px;
}
.closest_locations, .geo_header, .login {
  font-size: 18px;
  line-height: 16px;
  padding: 16px 6px;
  margin: 12px 5px 0;
  text-shadow: 1px 1px 5px #CCCCCC;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
.header_buttons .closest_locations,
.header_buttons .geo_header,
.header_buttons .login {
  padding: 10px 6px;
}
span.closest_locations, .login {
  float: left;
}
.login_alert a {
  text-decoration: none;
}
#header .login a:hover, #header .login_new a:hover, .login_alert a:hover {
  text-decoration: underline;
}
.login a, .login_new a, .login_alert a {
  color: #F53240;
}
span.why_login {
  font-size: 14px;
  color: #6b6b6b;
  line-height: 16px;
  display: block;
  padding: 5px 0 0;
}
#login .button {
  color: #EFEFEF;
  font-size: 18px;
  line-height: 15px;
  height: 36px;
  font-weight: bold;
  padding: 6px 6px;
  -webkit-border-radius: 6px;
  -moz-border-radius: 6px;
  border-radius: 6px;
  margin: 7px 0px;
  border: 2px solid #A9A9A9;
  background-color: #303030;
  -webkit-box-sizing: border-box; 
  -moz-box-sizing: border-box;    
  box-sizing: border-box;
  box-shadow: 1px 1px 3px #888;
  font-family: 'Freight-MedSC',Helvetica,Arial,sans-serif;
  text-transform: uppercase;
}
#login .button:hover {
  cursor: pointer;
  background: #A9A9A9;
}
#login .reset-password .button,
#login .confirmation .button {
  cursor: pointer;
  width: 260px;
  font-size: 12px;
}
#login .error {
  font-style: italic;
  color: red;
}
#login a {
  font-weight: bold;
  font-size: 18px;
}
div.pick_a_map .login_home {
  margin: 6px 5px;
  float: right;
}
.login_alert {
  text-transform: uppercase;
  background: #fff1f1;
}
.location_detail .login_alert {
  text-align: center;
}
.geo_header {
  float: left;
}
.region_name, .region_name_page, 
#header span.region_name_page a {
  color: #F53240;
  font-size: 20px;
  line-height: 22px;
  font-family: 'Freight-MedSC',Helvetica,Arial,sans-serif;
  text-align: center;
  -webkit-border-radius: 0;
  -moz-border-radius: 0;
  border-radius: 0;
  display: block;
  font-weight: bold;
}
.region_name_container {
  height: 45px !important;
  height: 170px;
  position: relative;
}
.region_name_inner {
  display: table !important; 
  position: absolute !important; 
  height: 45px;
  width: 160px;
  margin-left: 5px;
}
.region_name {
  padding: 12px 0 6px;
  height: 45px;
  display: table-cell !important;
  text-align: center;
  vertical-align: middle;
}
.other_regions {
  display: table !important;
  position: absolute !important;
  height: 50px;
  width: 164px;
  padding-top: 0;
}
span.region_name_page {
  display: table-cell !important;
  text-align: center;
  vertical-align: middle;
}

/** END Header **/

/** Lookup form **/

#operator_section_link, 
#zone_section_link, 
#machine_section_link, 
#city_section_link, 
#location_section_link, 
#type_section_link {
  font-size: 18px;
  background: #FBFBFB;
  border: 1px solid #312f2d;
  color: #303030;
  border-radius: 5px;
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
  -webkit-box-sizing: border-box; 
  -moz-box-sizing: border-box;    
  box-sizing: border-box;
  margin-left: 5px;
}
.section {
  padding: 7px 5px 3px;
  margin-top: 30px;
  height: 33px;
}
.search_field_container {
  width: 400px;
  height: 28px;
  border: 2px solid #A9A9A9;
  float: right;
  background: #FFFFFF;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  border-radius: 2px;
  -moz-border-radius: 2px;
  -webkit-border-radius: 2px;
}
#form .search_field_container .search,
#form .search_other_button input {
  border: none;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  height: 28px;
  margin: -2px -2px 0 0;
  -webkit-border-radius: 0;
  -moz-border-radius: 0;
  border-radius: 0;
  padding: 0 5px;
  background: #303030;
  color: #EFEFEF;
  font-weight: bold;
  float: right;
  box-shadow: none;
  border-bottom-right-radius: 2px;
  border-top-right-radius: 2px;
  -moz-border-bottom-right-radius: 2px;
  -moz-border-top-right-radius: 2px;
  -webkit-border-bottom-right-radius: 2px;
  -webkit-border-top-right-radius: 2px;
}
.search_other_button {
  float: right;
  margin: 2px 2px 0 0;
}
#form .search_field_container input.lookup_search_input {
  border: none;
  margin: 0;
  height: auto;
  padding-left: 4px;
}
.location_search {
  margin-left: 2px;
}
.location_search .info {
  font-family: Helvetica;
  font-weight: normal;
  margin: 0 5px;
  font-size: 12px;
}
#other_search_options_link:active {
   border-top-color: #484848;
   background: #484848;
}
#other_search_options {
  padding: 6px 0px 4px 6px;
  float: left;
  width: 100%;
}
#other_search_options a {
  padding: 0px 6px 2px;
  text-decoration: none;
} 
#other_search_options a:hover {
  background: #303030;
  text-decoration: none;
  border-color: #642501;
  color: #F53240 !important;
}
#locations p {
  margin-top: 20px;
  background-color: #EFEFEF;
  padding: 10px;
  border-radius: 5px;
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
  border: 1px solid #9fa7ae;
  font-family: "Palatino","Georgia","Times New Roman", Times,serif;
  font-weight: normal;
}
#locations .darkb {
  font-size: 18px;
}
#locations {
  margin-top: 10px;
}
#form {
  position: fixed;
  top: 49px;
  right: 13px;
  width: 630px;
  z-index: 20;
  border: 4px solid #EFEFEF;
  border-top: none;
  border-bottom-left-radius: 5px;
  border-bottom-right-radius: 5px;
  -moz-border-bottom-left-radius: 5px;
  -moz-border-bottom-right-radius: 5px;
  -webkit-border-bottom-left-radius: 5px;
  -webkit-border-bottom-right-radius: 5px;
  padding-top: 4px;
  background-color: #EFEFEF;
  box-shadow: 0 5px 10px rgba(90, 100, 117, 0.5);
}
#lookup_map, #container {
  width: 100%;
}
#lookup {
  border: 2px solid #303030;
  box-shadow: 0 4px 5px rgba(90, 100, 117, 0.3);
  display: inline;
  float: right;
  padding: 70px 10px 10px 10px;
  width: 600px;
  margin-right: 18px;
  position: relative;
  -webkit-transform: translateZ(0);
}
.search_results_header {
  margin: 20px 0 5px 13px;
  padding: 0 0 5px;
  text-transform: uppercase;
  color: #ebebeb;
  border-bottom: 1px solid white;
}
.location_link {
  cursor: pointer;
  font-size: 18px;
  font-weight: normal;
  padding: 2px 2px 2px;
  margin: 5px 0;
  text-align: center;
  background-color: #f3f2f2;
}
.location_link a {
  text-decoration: none;
}
.location_link a:hover {
  text-decoration: underline;
  color: #303030;
}
#by_type_id, #by_operator_id, #by_location_id, #by_machine_id, #by_zone_id, #by_city_id {
  width: 200px;
  font-size: 16px;
}
.active_section_link {
  background-color: #303030 !important;
  color: #d3ecff !important;
}
a.active_section_link:hover {
  background: #1C150B !important;
}

/** Search Results **/

#search_results {
  color: #180e03;
}
.search_result ul {
  border-left: 8px solid #303030;
  padding-left: 0px;
  margin-bottom: 0px;
  border-radius: 5px 0 0 5px;
  -moz-border-radius: 5px 0 0 5px;
  -webkit-border-radius: 5px 0 0 5px;
}
#search_results li {
  list-style-type: none;
  padding-left: 10px;
  padding-right: 5px;
}
.name {
  font-size: 24px;
  line-height: 24px;
  height: 100%;
  color: #433A3A;
  background-color: #d3ecff;
  padding: 10px 5px;
  border-radius: 0 5px 0 0;
  -moz-border-radius: 0 5px 0 0;
  -webkit-border-radius: 0 5px 0 0;
}
.address, .address_full {
  font-size: 16px;
  line-height: 16px;
  font-family: "Freight-MedSC",Helvetica,Arial,sans-serif;
}
li.address {
  padding: 2px 0 2px 5px;
  background: #EFEFEF;
}
li.address_full {
  padding: 0 0 0 5px;
  background: #EFEFEF;
  width: 435px;
}
a.website {
  float: left;
  width: 450px;
  font-family: "Palatino","Georgia","Times New Roman", Times,serif;
}
.machine_name {
  float: left;
  background-color: #303030;
  color: #ffffff;
  font-size: 26px;
  line-height: 28px;
  height: 100%;
  padding: 3px 15px 3px 5px;
  width: 100%;
  margin-left: -20px;
  font-family: "Freight-MedSC",Helvetica,Arial,sans-serif;
  font-weight: bold;
}
.machine_name:hover {
  background-color: #414141;
}
.machine_names {
  color: #171717;
